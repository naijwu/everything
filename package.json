{
  "name": "everything",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@9",
  "scripts": {
    "dev:api": "pnpm --filter @everything/api dev",
    "dev:scheduler": "pnpm --filter @everything/scheduler dev",
    "dev:workers": "pnpm -r --filter \"@everything/worker-*\" dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  }
}
